{
  "name": "copy-tab-info",
  "displayName": "Copy Tab Info",
  "version": "0.0.1",
  "description": "Copy current Tab title and url",
  "author": "Deer404",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.88.0",
    "punycode": "^2.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.7",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "activeTab",
      "clipboardWrite",
      "commands",
      "scripting"
    ],
    "commands": {
      "copy-tab-info": {
        "suggested_key": {
          "default": "Ctrl+Shift+P",
          "mac": "Command+Shift+P"
        },
        "description": "Copy Tab Information"
      },
      "copy-tab-info-markdown": {
        "suggested_key": {
          "default": "Ctrl+Shift+M",
          "mac": "Command+Shift+M"
        },
        "description": "Copy Tab Information as Markdown"
      }
    }
  }
}